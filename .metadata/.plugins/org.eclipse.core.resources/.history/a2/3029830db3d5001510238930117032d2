
public class Program {

	public static void main(String[] args) {
		
		Person p = new Person(); //создали переменную типа класса Person
								//и вызвали конструктор Person
		p.name = "Катя";
		p.age = 30;
		
		Person p2 = new Person();
		p2.name = "Маша";
		p2.age = 31;
		System.out.println(p2.name.toUpperCase());	
		
		p.show();
		p2.show();
	

	}

}
