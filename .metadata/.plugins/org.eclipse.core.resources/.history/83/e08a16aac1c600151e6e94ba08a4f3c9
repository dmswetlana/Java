import java.util.Scanner;
import static java.lang.System.out;

public class Crow {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		out.print("Введите число ворон на ветке : " );
		int  n = sc.nextInt();
		//если число ворон до бесконечности, то анализируем
		//последнюю цифру числа
		int n1 = n%10;
		
		//не сработает, если число оканчивается на 11, 12, 13
		//берём остаток от двух последних цифр
		int n2 = n%100;
		
		String v="";
		
		if (n2>=11 && n2<14)
			v = "ворон";
		else
		{
		switch(n1)
		{
		case 1:
			v ="ворона";
			break;
		case 2:
		case 3:
		case 4:
			v = "вороны";
			break;
		case 0:
		case 5:
		case 6:
		case 7:
		case 8:
		case 9:
			v="ворон";
			break; 
		}
		}
		String result = String.format("На ветке %d %s", n1,v);
		
		out.println(result);
	}

}
