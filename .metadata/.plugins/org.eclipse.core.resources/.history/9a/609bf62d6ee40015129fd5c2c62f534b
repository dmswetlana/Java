import static java.lang.System.out;

public class Program {

	public  void sayHello(){
		out.println("Привет");
	}
	
	public  void sayHello(String name){
		out.printf("Привет, %s\n", name);
	}
	
	public static void main(String[] args) {
		
		//sayHello();
		// Program.sayHello();
		Program p= new Program();
		p.sayHello();
		p.sayHello("Катя");

	}

}
