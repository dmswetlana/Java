public class Eucl {

	public static void main(String[] args) {
		int A = 234;
		int B = 122;
		int nod = B;

		for (int c = A % B, a = A, b = B; c != 0; c = a % b) {
			nod = c;
			a = b;
			b = c;
		}
		System.out.println(A / nod + "/" + B / nod);

	}

}
