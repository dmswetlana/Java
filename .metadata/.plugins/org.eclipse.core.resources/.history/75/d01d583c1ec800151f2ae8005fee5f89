// в этой программе объ€вл€ютс€ два объекта класса Box
class Box{
	double wigth;
	double heigth;
	double depth;
}

public class BoxDemo {

	public static void main(String[] args) {
		Box mybox1 = new Box();
		Box mybox2 = new Box();
		
		double vol;
		
		//присваиваем значени€ переменным класса mybox1
		mybox1.wigth = 10;
		mybox1.heigth = 20;
		mybox1.depth = 15;
		
		//присваиваем значени€ переменным класса mybox2
		mybox2.wigth = 3;
		mybox2.heigth = 6;
		mybox2.depth = 9;
		
		//объЄм первого параллелепипеда
		vol = mybox1.wigth * mybox1.heigth * mybox1.depth;
		System.out.println("ќбъем первого параллелепипеда = " + vol);
		
		//объЄм второго параллелепипеда
		vol = mybox2.wigth * mybox2.heigth * mybox2.depth;
		System.out.println("ќбъем второго параллелепипеда = " + vol);
				
		
		

	}

}
